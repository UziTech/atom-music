{
  "name": "atom-music",
  "main": "./lib/atom-music",
  "version": "0.10.2",
  "description": "A very simple Music player for atom, listen to your favorite music while workin in atom.",
  "keywords": [
    "music",
    "audio"
  ],
  "activationCommands": {
    "atom-workspace": [
      "atom-music:toggle",
      "atom-music:play-pause",
      "atom-music:toggle-shuffle",
      "atom-music:show-playlist",
      "atom-music:forward-15s",
      "atom-music:backward-15s",
      "atom-music:next-track",
      "atom-music:previous-track"
    ]
  },
  "deserializers": {
    "atom-music/AtomMusicView": "deserializeAtomMusicView"
  },
  "repository": "https://github.com/sdinesh86/atom-music",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "atom --test spec",
    "lint": "coffeelint ."
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0"
  },
  "devDependencies": {
    "@semantic-release/apm-config": "^8.0.0",
    "@coffeelint/cli": "^3.2.4",
    "semantic-release": "^17.0.7",
    "temp": "^0.9.1"
  }
}
